{
  "name": "Laravel Project with SQLite",
  "dockerFile": "Dockerfile",
  "extensions": [
    "mtxr.sqltools",
    "alexcvzz.vscode-sqlite"
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
